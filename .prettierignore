.env
.env.development.local
.env.test.local
.env.production.local
.env.local

# Ignore OS files:
.DS_Store
.git/
.hg/
.svn/

# Ignore editor configs:
.vscode/
.idea/

# Ignore common lock files:
package-lock.json
yarn.lock
